            }
            if ($('#contactPhone').val() == '') {
                $('#contactPhone').addClass("error");
                $('#btnContactSupport').prop('disabled', true)
            }
            if ($('#contactMessage').val() == '') {
                $('#contactMessage').addClass("error");
                $('#btnContactSupport').prop('disabled', true)
            }
            if ($('#contactCompanyName').val() != '' && $('#contactNumberEmployee').val() != '' && $('#contactContactName').val() != '' && $('#contactTitle').val() != '' && $('#contactEmail').val() != '' && $('#contactPhone').val() != '' && $('#contactMessage').val() != '') {
                var returnEmail = validateEmail2($('#contactEmail').val());
                if (returnEmail != false) {
                    SetDataTabelContactSupport();
                }
            }
        });
        function SetDataTabelContactSupport() {
            var datapost = $('#section-contact-us').serialize() + '&Pricing=' + pricingSelected;
            $.ajax({
                method: "POST",
                timeout: 60000,
                url: '@Url.Action("GetListContactSupportTable", "ContactSupport")',
                dataType: 'json',
                data: datapost,
                cache: false,
                context: document.body
            }).done(function (data) {
                if (data.errMessage == 0) {
                    $('.classGetPopUp').trigger('click');
                }
            });
        }
        // Opera 8.0+
        var isOpera = (!!window.opr && !!opr.addons) || !!window.opera || navigator.userAgent.indexOf(' OPR/') >= 0;
        // Firefox 1.0+
        var isFirefox = typeof InstallTrigger !== 'undefined';
        // Safari 3.0+ "[object HTMLElementConstructor]"
        var isSafari = /constructor/i.test(window.HTMLElement) || (function (p) { return p.toString() === "[object SafariRemoteNotification]"; })(!window['safari'] || (typeof safari !== 'undefined' && safari.pushNotification));
